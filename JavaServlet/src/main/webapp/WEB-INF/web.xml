<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>demoServlet</servlet-name>
		<servlet-class>com.company.javaservlet.MyFirstServlet</servlet-class>

	</servlet>

	<servlet-mapping>
		<servlet-name>demoServlet</servlet-name>
		<url-pattern>/demoServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>demoGenericServlet</servlet-name>
		<servlet-class>com.company.javaservlet.DemoGenericServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>demoGenericServlet</servlet-name>
		<url-pattern>/demoGeneric</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>demoHttpServlet</servlet-name>
		<servlet-class>com.company.javaservlet.HttpDemoServlet</servlet-class>

	</servlet>

	<servlet-mapping>
		<servlet-name>demoHttpServlet</servlet-name>
		<url-pattern>/demoHttpServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>s1</servlet-name>
		<servlet-class>com.company.javaservlet.HelloServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>s1</servlet-name>
		<url-pattern>/servlet1</url-pattern>
	</servlet-mapping>

	<filter>
		<filter-name>f1</filter-name>
		<filter-class>com.company.javaservlet.MyFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>f1</filter-name>
		<url-pattern>/servlet1</url-pattern>
	</filter-mapping>

	<listener>
		<listener-class>com.company.javaservlet.MyServletContextListener</listener-class>
	</listener>

	<listener>
		<listener-class>com.company.javaservlet.MyServletContextListener2</listener-class>
	</listener>


</web-app>
